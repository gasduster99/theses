\begin{frame}{Expected Improvement (EI)}
\begin{minipage}[h!]{0.49\textwidth}
\begin{align*}
	f_{min} &= \min\Big\{ f(\bm{x_1}), ..., f(\bm{x_{N}}) \Big\}\\
        \ix &= \max \Big\{ \big(f_{min} - f(\bm{x})\big), ~0 \Big\}\\
        \text{EI} &= \Eix
\end{align*}
\end{minipage}
